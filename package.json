{
  "name": "@geoplateforme/geocodeur",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "api:start": "node api",
    "worker:start": "node batch/worker.js",
    "lint": "xo",
    "test": "c8 --all ava --no-worker-threads api batch indexes lib",
    "test-lcov": "c8 --reporter=lcov --reporter=text --reporter=html --all ava --no-worker-threads api batch indexes lib",
    "test-recette": "ava tests/recette",
    "test-recette-batch": "ava --timeout=10m --serial tests/recette-batch",
    "poi:build-from-bdtopo": "node indexes/poi/scripts/build-from-bdtopo",
    "poi:build-index": "node indexes/poi/scripts/build-index",
    "poi:publish-index": "node indexes/poi/scripts/publish-index",
    "poi:download-index": "node indexes/poi/scripts/download-index",
    "poi:start": "node indexes/poi/service",
    "parcel:build-index": "node indexes/parcel/scripts/build-index",
    "parcel:publish-index": "node indexes/parcel/scripts/publish-index",
    "parcel:download-index": "node indexes/parcel/scripts/download-index",
    "parcel:start": "node indexes/parcel/service",
    "address:build-index": "node indexes/address/scripts/build-index",
    "address:publish-index": "node indexes/address/scripts/publish-index",
    "address:download-index": "node indexes/address/scripts/download-index",
    "address:start": "node indexes/address/service"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.665.0",
    "@aws-sdk/lib-storage": "^3.665.0",
    "@etalab/decoupage-administratif": "^4.0.0",
    "@livingdata/tabular-data-helpers": "^0.0.11",
    "@mapbox/geojsonhint": "^3.3.0",
    "@mapbox/tilebelt": "^1.0.2",
    "@turf/bbox": "^6.5.0",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/boolean-intersects": "^6.5.0",
    "@turf/centroid": "^6.5.0",
    "@turf/circle": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "@turf/truncate": "^6.5.0",
    "JSONStream": "^1.3.5",
    "addok-cluster": "^0.8.0",
    "content-disposition": "^0.5.4",
    "cors": "^2.8.5",
    "csv-write-stream": "^2.0.0",
    "date-fns": "^4.1.0",
    "date-time": "^4.0.0",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "express": "^4.19.2",
    "fast-glob": "^3.3.2",
    "flatbush": "^4.4.0",
    "flexsearch": "^0.7.31",
    "gdal-async": "^3.9.0",
    "geobuf": "^3.0.2",
    "got": "^14.4.1",
    "http-errors": "^2.0.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "lmdb": "^3.0.12",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "msgpackr": "^1.10.2",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "natural": "^6.10.4",
    "ndjson": "^2.0.0",
    "nock": "^13.5.4",
    "on-finished": "^2.4.1",
    "p-event": "^6.0.1",
    "p-filter": "^4.1.0",
    "p-limit": "^6.1.0",
    "p-retry": "^6.2.0",
    "pbf": "^3.3.0",
    "pumpify": "^2.0.1",
    "tar-fs": "^3.0.6"
  },
  "devDependencies": {
    "ava": "^6.1.3",
    "c8": "^10.1.2",
    "get-stream": "^9.0.1",
    "mock-fs": "^5.2.0",
    "papaparse": "~5.3.2",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "xo": "^0.58.0"
  },
  "c8": {
    "exclude": [
      "**/__tests__/*.js",
      "api/index.js",
      "**/service/index.js",
      "batch/storage/fs.js",
      "tests",
      "coverage"
    ]
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "unicorn/prevent-abbreviations": "off",
      "import/no-unassigned-import": "off",
      "import/order": "off",
      "object-curly-newline": "off"
    }
  },
  "engines": {
    "node": ">= 20.9 <21"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
